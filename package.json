{
    "name": "obsidian-github-publisher",
    "version": "6.0.0-2",
    "description": "Github Publisher is a plugin that help you to send file in a configured Github Repository, based on a frontmatter entry state.",
    "main": "main.js",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "lint": "eslint --ext .ts src/",
        "lint:fix": "eslint --ext .ts,.js,.json src/ --fix",
        "lint:json": "eslint --ext .json . --fix",
        "build": "obsidian-plugin build src/main.ts -S src/styles.css ",
        "devprod": "node dev.js --prod",
        "dev": "node dev.js",
        "preexport": "pn run build",
        "export": "node export.js",
        "bump": "node commit-and-tag-version.js",
        "postbump": "git push --follow-tags origin master",
        "preupgrade": "pn run bump",
        "upgrade": "pn run export",
        "test": "jest"
    },
    "commit-and-tag-version": {
        "t": ""
    },
    "keywords": [],
    "author": "Mara",
    "license": "AGPL-3.0",
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@types/node": "^18.11.10",
        "@typescript-eslint/eslint-plugin": "^5.45.1",
        "@typescript-eslint/parser": "^5.45.1",
        "babel-jest": "^29.3.1",
        "builtin-modules": "^3.3.0",
        "esbuild": "latest",
        "eslint": "^8.34.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsonc": "^2.6.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.3.1",
        "obsidian": "^1.2.8",
        "obsidian-dataview": "latest",
        "prettier": "^2.8.0",
        "ts-jest": "^29.0.3",
        "tslib": "latest",
        "typescript": "latest"
    },
    "dependencies": {
        "@octokit/core": "^4.1.0",
        "ansi-colors": "^4.1.3",
        "commander": "^10.0.1",
        "commit-and-tag-version": "latest",
        "detect-indent": "^7.0.1",
        "detect-newline": "^4.0.0",
        "dotenv": "^16.0.3",
        "i18next": "^22.4.10",
        "js-base64": "^3.7.3",
        "obsidian-plugin-cli": "^0.9.0",
        "release-please": "latest",
        "slugify": "^1.6.6",
        "standard-version": "^9.5.0",
        "stringify-package": "^1.0.1"
    }
}
