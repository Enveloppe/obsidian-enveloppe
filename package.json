{
    "name": "obsidian-github-publisher",
    "version": "6.1.0-3",
    "description": "Github Publisher is a plugin that help you to send file in a configured Github Repository, based on a frontmatter entry state.",
    "main": "main.js",
    "scripts": {
        "lint:fix": "eslint --ext .ts,.js,.json src/ --fix",
        "build": "obsidian-plugin build src/main.ts -S src/styles.css ",
        "dev:prod": "node dev.js --prod",
        "dev": "node dev.js",
        "preexport": "pnpm run build",
        "export": "node export.js",
        "bump": "node commit-and-tag-version.js",
        "postbump": "git push --follow-tags origin main",
        "predeploy": "pnpm run bump",
        "deploy": "pnpm run export",
        "test": "jest"
    },
    "commit-and-tag-version": {
        "t": ""
    },
    "keywords": [],
    "author": "Mara",
    "license": "AGPL-3.0",
    "devDependencies": {
        "@babel/core": "^7.22.1",
        "@babel/preset-env": "^7.22.4",
        "@babel/preset-typescript": "^7.21.5",
        "@octokit/core": "^4.2.1",
        "@types/node": "^18.16.16",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "@typescript-eslint/parser": "^5.59.9",
        "ansi-colors": "^4.1.3",
        "babel-jest": "^29.5.0",
        "builtin-modules": "^3.3.0",
        "commander": "^10.0.1",
        "commit-and-tag-version": "latest",
        "detect-indent": "^7.0.1",
        "detect-newline": "^4.0.0",
        "dotenv": "^16.1.4",
        "esbuild": "latest",
        "eslint": "^8.42.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsonc": "^2.8.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "i18next": "^22.5.1",
        "jest": "^29.5.0",
        "js-base64": "^3.7.5",
        "obsidian": "^1.2.8",
        "obsidian-dataview": "latest",
        "obsidian-plugin-cli": "^0.9.0",
        "prettier": "^2.8.8",
        "slugify": "^1.6.6",
        "standard-version": "^9.5.0",
        "ts-jest": "^29.1.0",
        "tslib": "latest",
        "typescript": "latest"
    },
    "dependencies": {
        "dedent": "^0.7.0"
    }
}
