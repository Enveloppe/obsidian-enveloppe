{
  "name": "obsidian-enveloppe",
  "version": "7.6.4",
  "description": "Github Publisher is a plugin that help you to send file in a configured Github Repository, based on a frontmatter entry state.",
  "main": "main.js",
  "private": true,
  "scripts": {
    "lint": "biome format --write src/",
    "prebuild": "tsc --noEmit --skipLibCheck",
    "build": "node esbuild.config.mjs --production",
    "dev:prod": "node esbuild.config.mjs --vault",
    "dev": "node esbuild.config.mjs",
    "export": "node esbuild.config.mjs --production --vault",
    "prerelease": "bun run prebuild",
    "release": "node commit-and-tag-version.mjs",
    "postrelease": "git push --follow-tags origin master",
    "predeploy": "bun run bump",
    "deploy": "bun run export",
    "sync:locale": "bun run hooks/update-translation.mjs pull",
    "prepare": "husky install"
  },
  "commit-and-tag-version": {
    "t": ""
  },
  "keywords": [],
  "author": "Mara",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@biomejs/biome": "^2.2.3",
    "@codemirror/buildhelper": "^1.0.2",
    "@codemirror/language": "github:lishid/cm-language",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.2",
    "@enveloppe/obsidian-dataview": "^0.5.70-2",
    "@octokit/core": "^7.0.3",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.3.1",
    "@types/obsidian-typings": "npm:obsidian-typings@^4.50.0",
    "ansi-colors": "^4.1.3",
    "builtin-modules": "^5.0.0",
    "commander": "^14.0.0",
    "commit-and-tag-version": "^12.6.0",
    "cross-env": "^10.0.0",
    "detect-indent": "^7.0.1",
    "detect-newline": "^4.0.1",
    "dotenv": "^17.2.2",
    "esbuild": "^0.25.9",
    "husky": "^9.1.7",
    "i18next": "^25.5.2",
    "js-base64": "^3.7.8",
    "klona": "^2.0.6",
    "obsidian": "^1.8.7",
    "prettier": "^3.6.2",
    "slugify": "^1.6.6",
    "ts-dedent": "^2.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "cross-var": "^1.1.0",
    "dedent": "^1.7.0",
    "dotenv-cli": "^10.0.0",
    "env-cmd": "^11.0.0",
    "ts-deepmerge": "^7.0.3",
    "tslog": "^4.9.3"
  },
  "pnpm": {
    "overrides": {
      "svelte@<3.49.0": ">=3.49.0",
      "got@<11.8.5": ">=11.8.5"
    }
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@codemirror/language",
    "@types/obsidian-typings",
    "core-js",
    "electron",
    "esbuild"
  ]
}
