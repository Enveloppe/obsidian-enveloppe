{
	"name": "obsidian-enveloppe",
	"version": "7.5.13-3",
	"description": "Github Publisher is a plugin that help you to send file in a configured Github Repository, based on a frontmatter entry state.",
	"main": "main.js",
	"private": true,
	"scripts": {
		"lint": "biome format --write src/",
		"prebuild": "tsc --noEmit --skipLibCheck",
		"build": "node esbuild.config.mjs --production",
		"dev:prod": "node esbuild.config.mjs --vault",
		"dev": "node esbuild.config.mjs",
		"export": "node esbuild.config.mjs --production --vault",
		"prerelease": "bun run prebuild",
		"release": "node commit-and-tag-version.mjs",
		"postrelease": "git push --follow-tags origin master",
		"predeploy": "bun run bump",
		"deploy": "bun run export",
		"sync:locale": "bun run hooks/update-translation.mjs",
		"prepare": "husky install"
	},
	"commit-and-tag-version": {
		"t": ""
	},
	"keywords": [],
	"author": "Mara",
	"license": "AGPL-3.0",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@codemirror/buildhelper": "^1.0.2",
		"@codemirror/language": "github:lishid/cm-language",
		"@codemirror/state": "^6.5.1",
		"@codemirror/view": "^6.36.2",
		"@enveloppe/obsidian-dataview": "^0.5.67",
		"@octokit/core": "^6.1.3",
		"@types/luxon": "^3.4.2",
		"@types/node": "^22.10.7",
		"@types/obsidian-typings": "npm:obsidian-typings@^2.14.6",
		"ansi-colors": "^4.1.3",
		"builtin-modules": "^4.0.0",
		"commander": "^13.1.0",
		"commit-and-tag-version": "^12.5.0",
		"cross-env": "^7.0.3",
		"detect-indent": "^7.0.1",
		"detect-newline": "^4.0.1",
		"dotenv": "^16.4.7",
		"esbuild": "^0.24.2",
		"husky": "^8.0.0",
		"i18next": "^24.2.1",
		"js-base64": "^3.7.7",
		"klona": "^2.0.6",
		"obsidian": "^1.7.2",
		"prettier": "^3.4.2",
		"slugify": "^1.6.6",
		"ts-dedent": "^2.2.0",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"cross-var": "^1.1.0",
		"dedent": "^1.5.3",
		"dotenv-cli": "^8.0.0",
		"env-cmd": "^10.1.0",
		"ts-deepmerge": "^7.0.2",
		"tslog": "^4.9.3"
	},
	"peerDependencies": {
		"@types/electron": "npm:@ophidian/electron-types@^24.3.1"
	},
	"pnpm": {
		"overrides": {
			"svelte@<3.49.0": ">=3.49.0",
			"got@<11.8.5": ">=11.8.5"
		}
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"@codemirror/language",
		"core-js",
		"electron",
		"esbuild"
	]
}
